{
  "name": "svgmorph",
  "version": "0.0.1",
  "description": "Prototype for animating SVGs",
  "author": "Curtis Dulmage",
  "license": "ISC",
  "keywords": [
    "SVG",
    "Morph",
    "Animation"
  ],
  "homepage": "https://github.com/beefchimi/svgmorph#readme",
  "bugs": {
    "url": "https://github.com/beefchimi/svgmorph/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beefchimi/svgmorph.git"
  },
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.4.0",
    "npm": ">=10.8.0"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules/.cache && rm -rf node_modules/.vite",
    "nuke": "npm run clean && rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force",
    "start": "vite",
    "preview": "vite preview",
    "build": "npm run clean && tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "echo 'There is no testing suite set up for this app.'"
  },
  "dependencies": {
    "beeftools": "^0.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "^62.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-svgr": "^4.2.0"
  }
}
